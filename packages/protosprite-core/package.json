{
  "name": "protosprite-core",
  "type": "module",
  "version": "0.0.1",
  "description": "A compact, protobuf based sprite exchange format",
  "main": "dist/core.js",
  "types": "dist/src/core/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/core.js",
      "types": "./dist/src/core/index.d.ts"
    },
    "./importers/aseprite": {
      "default": "./dist/importers/aseprite.js",
      "types": "./dist/src/importers/aseprite.d.ts"
    },
    "./transform": {
      "default": "./dist/transform.js",
      "types": "./dist/src/transform/index.d.ts"
    }
  },
  "sideEffects": false,
  "keywords": [
    "protobuf",
    "sprites",
    "sprite-sheets"
  ],
  "author": "Robert Brownstein",
  "license": "MIT",
  "scripts": {
    "build:clean": "rm -rf ./proto_dist ./dist",
    "build:buf": "buf generate",
    "build:rollup": "rollup -c",
    "build": "npm run build:clean && npm run build:buf && npm run build:rollup",
    "prepublish": "npm run build"
  },
  "directories": {
    "src": "src",
    "dist": "dist",
    "proto_dist": "proto_dist"
  },
  "files": [
    "src",
    "dist",
    "proto_dist"
  ],
  "dependencies": {
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "js-base64": "^3.7.8"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.56.0",
    "@bufbuild/protobuf": "^2.7.0",
    "@bufbuild/protoc-gen-es": "^2.7.0",
    "@kayahr/aseprite": "^2.0.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "bin-pack": "^1.0.2",
    "jest": "^29.7.0",
    "jimp": "^1.6.0",
    "rollup": "^4.48.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "@kayahr/aseprite": "^2.0.0",
    "bin-pack": "^1.0.2",
    "jimp": "^1.6.0"
  }
}
